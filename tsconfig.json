{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": false,
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*",
                "src/types/*"
            ]
        }
    },
    "include": [
       "distribucionGrupos/*", "logic/*", "test/test.js", "test/pruebaDistribucionGrupos.js"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}